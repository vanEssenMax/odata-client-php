{
    "name": "justbetter/odata-client",
    "description": "Saint Systems OData Client for PHP",
    "keywords": ["odata", "rest", "php"],
    "homepage": "https://github.com/justbetter/odata-client-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saint Systems",
            "email": "contact@saintsystems.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "nesbot/carbon": "^3.1.1",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "files": [
            "src/Core/helpers.php"
        ],
        "psr-4": {
            "SaintSystems\\OData\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
